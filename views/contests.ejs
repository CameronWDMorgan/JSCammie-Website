<!-- setup basic html template with metadata etc: -->
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Creative Contests & Competitions | JSCammie.com</title>
    <meta name="description" content="Join JSCammie's creative contests and competitions! Participate in AI art challenges, win prizes, and showcase your creativity with our free AI image generator.">
    <meta name="keywords" content="jscammie contests, ai art contest, creative competition, ai image generator contest, art challenge, prizes, community">
    <meta name="author" content="JSCammie">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="https://jscammie.com/contests">
    
    <link rel='stylesheet' href='/style.css'/>

    <%- include('../partials/_headerAll') %>
    
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-K3KJJKS1R4"></script>
    <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'G-K3KJJKS1R4');
    </script>

    <!-- discord web embed:  -->
    <meta property="og:title" content="Creative Contests & Competitions | JSCammie.com" />
    <meta property="og:description" content="Join JSCammie's creative contests and competitions! Participate in AI art challenges, win prizes, and showcase your creativity with our free AI image generator." />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://jscammie.com/contests" />
    <meta property="og:site_name" content="JSCammie.com" />
    
    <!-- Twitter Card meta tags -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:title" content="Creative Contests & Competitions | JSCammie.com">
    <meta name="twitter:description" content="Join JSCammie's creative contests and competitions! Participate in AI art challenges, win prizes, and showcase your creativity with our free AI image generator.">

</head>
<body>

    <!-- get partials/_navigation: -->
    <%- include('../partials/_navigation') %>

    <style>
        .screenLine {
            width: 100%;
            height: 4px;
            background-color: #ffffff;
            margin: 10px 0px;
            border-radius: 10px;
        }
    </style>

    <style>
        a {
            color: #be6aff;
            text-decoration: none;
        }
        a:hover {
            text-decoration: underline;
        }
    </style>

    <div class="center-fully-no-flex" style="margin-bottom: 1rem;">
        <%- include('../partials/_bannerAdvert') %>
    </div>

    <div class="round-frosted page-content mw50-dynamic" style="padding: 40px; justify-content: center;">
        <h1>JSCammie Contests</h1>
        <p>Be sure to read through the guidelines listed below!</p>
        <%- contestGuidelines %>
        <% contests.forEach(function(contestInfo) { %>
            <div class="screenLine"></div>
            <%- contestInfo %>
        <% }); %>

    </div>

    <!-- get partials/_footer: -->
    <%- include('../partials/_footer') %>
    
</body>
</html>