                        stepsInput.min = min
                        stepsInput.max = max
                    }

                    function maxQuantity(max) {
                        quantity.max = max
                        quantityInput.max = max
                    }

                    function maxCfguidance(max) {
                        cfguidance.max = max
                        cfguidanceInput.max = max
                    }

                    if (model.value.includes('sdxl')) {
