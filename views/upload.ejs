<!-- setup basic html template with metadata etc: -->
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Upload Content & Models | JSCammie.com</title>
    <meta name="description" content="Upload your AI generated content and models to JSCammie.com. Share your creations with the community and contribute to the platform.">
    <meta name="keywords" content="upload content, upload model, share ai art, contribute content, jscammie upload, community sharing">
    <meta name="author" content="JSCammie">
    
    <!-- Open Graph meta tags for social media -->
    <meta property="og:title" content="Upload Content & Models | JSCammie.com">
    <meta property="og:description" content="Upload your AI generated content and models to JSCammie.com. Share your creations with the community and contribute to the platform.">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://jscammie.com/upload">
    <meta property="og:site_name" content="JSCammie.com">
    
    <!-- Twitter Card meta tags -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:title" content="Upload Content & Models | JSCammie.com">
    <meta name="twitter:description" content="Upload your AI generated content and models to JSCammie.com. Share your creations with the community and contribute to the platform.">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="https://jscammie.com/upload">
    
    <!-- Robots directive for upload pages -->
    <meta name="robots" content="noindex, follow">
    
    <link rel='stylesheet' href='/style.css'/>
    
    <%- include('../partials/_headerAll') %>
    
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-K3KJJKS1R4"></script>
    <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'G-K3KJJKS1R4');
    </script>
    
</head>
<body>

    <!-- get partials/_navigation: -->
    <%- include('../partials/_navigation') %>

    <div class="center-fully-no-flex" style="margin-bottom: 1rem;">
        <%- include('../partials/_bannerAdvert') %>
    </div>

    <div class="round-frosted page-content mw50-dynamic" style="padding: 40px; justify-content: center;">
        <div>
            <!-- tab toggle between showing content upload and model upload: -->
            <div class="tab">
                <button class="tablinks" onclick="openTab(event, 'content')">Content</button>
                <button class="tablinks" onclick="openTab(event, 'model')">Model</button>
            </div>
            
            <!-- script: -->
            <script>
                function openTab(evt, tabName) {
                    var i, tabcontent, tablinks;
                    tabcontent = document.getElementsByClassName("tabcontent");
                    for (i = 0; i < tabcontent.length; i++) {
                        tabcontent[i].style.display = "none";
                    }
                    tablinks = document.getElementsByClassName("tablinks");
                    for (i = 0; i < tablinks.length; i++) {
                        tablinks[i].className = tablinks[i].className.replace(" active", "");
                    }
                    document.getElementById(tabName).style.display = "block";
                    evt.currentTarget.className += " active";
                }
            </script>

            <div>
                <!-- content upload: -->
                <div id="content" class="tabcontent">
                    <h3>Content</h3>
                    <form action="/upload" method="post" enctype="multipart/form-data">
                        <input type="file" name="content" accept="image/*" required>
                        <input type="file" name="aidata" accept=".aidata" required>
                        <input type="submit" value="Upload">
                    </form>
                </div>
            </div>

        </div>
    </div>

    <!-- get partials/_footer: -->
    <%- include('../partials/_footer') %>
    
</body>
</html>